RewriteEngine On
#RewriteCond %{QUERY_STRING} ^address=([^&]*)\&name=([^&]*)\&code=([^&]*)$
#RewriteRule ^index\.php/news/newsletter/$ /index.php/news/newsletter/%3/%1/%2/? [R=301]
RewriteCond %{QUERY_STRING} ^C=(.*)$
RewriteRule ^index\.php/music/$ /index.php/music/? [R=301]

